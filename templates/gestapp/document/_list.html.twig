<h4><i class="fa-duotone fa-file-signature"></i> Documents</h4>
<div class="row">
    {% for document in documents %}
    <div class="col-1">
        <div class="card">
            <div class="card-body">
                {% if document.typeDoc == 'Pdf' %}
                <img src="" alt="">
                {% elseif document.typeDoc == 'Word' %}
                <img src="" alt="">
                {% elseif document.typeDoc == 'Excel' %}
                <img src="" alt="">
                {% elseif document.typeDoc == 'Mp4' %}
                <img src="" alt="">
                {% endif %}
                {{ document.name }}
            </div>
        </div>
    </div>
    {% else %}
    <div class="col-1">
        <div class="card">
            <div class="card-body">
                Pas de documents
            </div>
        </div>
    </div>
    {% endfor %}
</div>