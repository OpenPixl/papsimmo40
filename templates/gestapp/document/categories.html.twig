<div class="row justify-content-center">
    {% for categorie in categories %}
        <div class="col-2">
            <a class="btn btn-outline-primary btn-sm w-100 btnCat" href="{{ path('op_gestapp_document_categorie', {'idcat': categorie.id}) }}">
                {{ categorie.name|upper }}
            </a>
        </div>
    {% else %}
        <div class="col-12">
            pas de catégorie enregistrée
        </div>
    {% endfor %}
</div>

