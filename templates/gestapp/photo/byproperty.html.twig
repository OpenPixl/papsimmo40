<section class="mt-3">
    <h3>Les photos du bien</h3>
</section>
<section>
    <div class="row">
        <div class="col-3">
            {{ render(controller('App\\Controller\\Gestapp\\PhotoController::new', {'idproperty': property.id})) }}
        </div>
        <div class="col-9">
            <div class="row g-1" id="listephoto">
                {% include('gestapp/photo/_listephoto.html.twig') %}
            </div>
        </div>
    </div>
</section>
<div class="row mt-2">
    <div class="col-12">
        <div class="mt-1">
            {% if property.isIncreating == 1 %}
                <a id="stepBack" href="#" class="btn btn-sm btn-outline-dark disabled"><i class="fa-solid fa-backward-step"></i> Retour</a>
            {% else %}
                <a id="previousGallery" href="#" class="btn btn-sm btn-outline-dark"><i class="fa-solid fa-backward-step"></i> Retour</a>
            {% endif %}
            <button type="submit" id="stepGallery" class="btn btn-sm btn-outline-primary">{{ button_label|default("Valider et passer à l'étape vendeurs") }}</button>
        </div>
    </div>
</div>




