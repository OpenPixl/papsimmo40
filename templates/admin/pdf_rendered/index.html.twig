{% extends 'base.html.twig' %}

{% block title %}PdfRendered index{% endblock %}

{% block body %}
    <h1>PdfRendered index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Filename</th>
                <th>CreatedAt</th>
                <th>UpdatedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for pdf_rendered in pdf_rendereds %}
            <tr>
                <td>{{ pdf_rendered.id }}</td>
                <td>{{ pdf_rendered.name }}</td>
                <td>{{ pdf_rendered.filename }}</td>
                <td>{{ pdf_rendered.createdAt ? pdf_rendered.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ pdf_rendered.updatedAt ? pdf_rendered.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_admin_pdf_rendered_show', {'id': pdf_rendered.id}) }}">show</a>
                    <a href="{{ path('app_admin_pdf_rendered_edit', {'id': pdf_rendered.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_admin_pdf_rendered_new') }}">Create new</a>
{% endblock %}
